{
  "name": "browser-detect",
  "version": "0.2.22",
  "description": "Simplify detecting your browser.",
  "main": "dist/browser-detect.js",
  "scripts": {
    "build": "webpack --config webpack.config.babel.js",
    "lint": "tslint -c tslint.json src/**/*.ts",
    "test": "nyc mocha",
    "coverage": "nyc report --reporter=text-lcov > ./coverage/coverage.lcov && cat ./coverage/coverage.lcov | npx coveralls && npx codecov -t 5be80c5d-5d6f-4566-b3ee-338446ccb3cd && CODECLIMATE_REPO_TOKEN=3638311d28b61d4720a6e9b45382e55bbb04c78535e1684c6c5b74462a38b9fd codeclimate-test-reporter < ./coverage/coverage.lcov"
  },
  "nyc": {
    "all": true,
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "coverage/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/KennethanCeyer/browser-detect.git"
  },
  "keywords": [
    "browser",
    "agent",
    "modernizr",
    "detect",
    "node",
    "client"
  ],
  "author": "PIGNOSE <kennethan@nhpcw.com>",
  "license": "MIT",
  "types": "./types/browser-detect.d.ts",
  "typings": "./types/browser-detect.d.ts",
  "bugs": {
    "url": "https://github.com/KennethanCeyer/browser-detect/issues"
  },
  "homepage": "https://github.com/KennethanCeyer/browser-detect",
  "devDependencies": {
    "@types/chai": "^4.0.8",
    "@types/mocha": "^2.2.44",
    "@types/node": "^8.0.55",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "codeclimate-test-reporter": "^0.5.0",
    "codecov": "^3.0.0",
    "coveralls": "^3.0.0",
    "dts-gen": "^0.5.7",
    "mocha": "^4.0.1",
    "nyc": "^11.3.0",
    "ts-loader": "^3.2.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "tslint-config-airbnb": "^5.4.2",
    "typescript": "^2.6.2",
    "webpack": "^3.10.0"
  },
  "dependencies": {
    "core-js": "^2.5.1"
  }
}
