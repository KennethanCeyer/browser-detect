{
    "defaultSeverity": "error",
    "extends": [
        "tslint-config-airbnb"
    ],
    "jsRules": {},
    "rules": {
        "object-literal-key-quotes": false,
        "indent": [true, "spaces", 4],
        "import-name": false,
        "ter-indent": [true, 4],
        "max-line-length": [true, 120],
        "no-boolean-literal-compare": false,
        "strict-boolean-expressions": false
    },
    "rulesDirectory": []
}